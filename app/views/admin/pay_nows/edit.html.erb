<h1>Editing QuickPay</h1>
<%= render "shared/error_messages", :target => @pay_now %>
<%= form_for(@pay_now, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
    <table>
      <tr>
        <td>State:</td>
        <td><%= @pay_now.payment_state %></td>
      </tr>
    </table>

  <%= render :partial => "admin/shared/edit_resource_links" %>
<% end %>
